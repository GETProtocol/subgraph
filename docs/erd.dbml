// Enums

Enum Interaction {
  MINT
  SCAN
  CLAIM
}

// Entities

Table Protocol {
  id id
  getDebitedFromSilos bigint
  getCreditedToDepot bigint
  getMovedToFeeCollector bigint
  averageGetPerMint bigdecimal
  mintCount bigint
  invalidateCount bigint
  scanCount bigint
  checkInCount bigint
  claimCount bigint
}

Table ProtocolDay {
  id id
  day int
  getDebitedFromSilos bigint
  getCreditedToDepot bigint
  getMovedToFeeCollector bigint
  averageGetPerMint bigdecimal
  mintCount bigint
  invalidateCount bigint
  scanCount bigint
  checkInCount bigint
  claimCount bigint
}


Table Relayer {
  id id
  getDebitedFromSilo bigint
  getCreditedToDepot bigint
  averageGetPerMint bigdecimal
  mintCount bigint
  invalidateCount bigint
  checkInCount bigint
  scanCount bigint
  claimCount bigint
  events Event[] [ref: > Event.relayer]
  relayerDays RelayerDay[] [ref: > RelayerDay.relayer]
  usageEvents UsageEvent[] [ref: > UsageEvent.relayer]
}

Table RelayerDay {
  id id
  relayer Relayer
  day int
  getDebitedFromSilo bigint
  getCreditedToDepot bigint
  averageGetPerMint bigdecimal
  mintCount bigint
  invalidateCount bigint
  checkInCount bigint
  scanCount bigint
  claimCount bigint
}

Table Event {
  id id
  relayer Relayer
  usageEvents UsageEvent[] [ref: > UsageEvent.event]
  getDebitedFromSilo bigint
  getCreditedToDepot bigint
  averageGetPerMint bigdecimal
  mintCount bigint
  invalidateCount bigint
  checkInCount bigint
  scanCount bigint
  claimCount bigint
  eventName string
  shopUrl string
  imageUrl string
  orderTime bigint
  latitude bigdecimal
  longitude bigdecimal
}

Table UsageEvent {
  id id
  relayer Relayer
  blockNumber bigint
  blockTimestamp bigint
  orderTime bigint
  day int
  getDebitedFromSilo bigint
  getCreditedToDepot bigint
  interaction Interaction
  event event
  nftIndex bigint
  latitude bigdecimal
  longitude bigdecimal
}