{
  "name": "get-protocol-subgraph",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ get-protocol-subgraph",
    "create-local": "graph create --node http://localhost:8020/ get-protocol-subgraph",
    "remove-local": "graph remove --node http://localhost:8020/ get-protocol-subgraph",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 get-protocol-subgraph",
    "prepare": "husky install",
    "check": "yarn run lint && yarn run format --check",
    "lint": "eslint .",
    "format": "prettier --write --ignore-path .gitignore ."
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.21.1",
    "@graphprotocol/graph-ts": "0.20.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{graphql,md,ts}": "prettier --write"
  }
}
